		<h5>Date</h5>
		
		<input type='text' name = "datetime" placeholder='mm/dd/yyyy HH:MM' class='form-control'/>
			
		
		<h5>Location</h5>
		<table>
			<tr>
				<td>
					<input type='text' name ="location" placeholder='Location' class='form-control'/>
				</td>
				<td style='padding-left: 5px;'>
					<input type='button' value='Search Locations' class='form-control'/>
				</td>
			</tr>
		</table>
		
		<h5>Project*</h5>
		<table>
			<tr>
				<td>
					<input type='text' name = 'project' placeholder='Project' class='form-control'/>
				</td>
				<td style='padding-left: 5px;'>
					<input type='button' value='Search Projects' class='form-control'/>
				</td>
			</tr>
		</table>

		<h5>Agenda</h5>
		<table class='fullWidth'>
			<tr>
				<td style='width: 70%'>
					<input type='url' name = 'agenda' class='fullWidth' placeholder='Agenda Title' />
				</td>
				<td style='width: 30%'>
					<input type='button' value='Search Agendas' />
				</td>
			</tr>
		</table>
		
