<form name="input" id = "createMeeting" onsubmit="return validateForm()" action ="http://csse371-04.csse.rose-hulman.edu/Meeting/" method = 'post'>
		
		<h5>Topic</h5>
		<input type='text' name = "title" class='form-control'/>
		
		<h5>Description*</h5>
		<textarea class='form-control'></textarea>
		
		<h5>Attendees</h5>
		<select multiple id='attendees' size='6' class='form-control'>
			<option disabled>Select multiple by holding Ctrl/CMD</option>
			<option value='1'>Santa Claus</option>
			<option value='2'>WasARebel</option>
			<option value='3'>KemoSabe</option>
			<option value='4'>CaptainKirk</option>
			<option value='5'>Spock</option>
		</select>
