		<!-- <div id = 'content' style = 'width: 50%; float:right'> -->
		<h5>Tasks:</h5>
		<input type='text' name = 'task' placeholder='Task' class='form-control'/>
		<select>
			<option>Assigned User</option>
			<option>------------</option>
			<option>other users here</option>
		</select>
