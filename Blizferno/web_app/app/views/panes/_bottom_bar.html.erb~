<script>
	$("#mypost").click(function(){
	 $.post( "http://csse-04.csse.rose-hulman.edu/index?method=register", '{ "user": "test123", "pass": "123" }');
		});
</script>



<script>
	/*$("#mybutton").click(function(){
		$.ajax({
			type: 'POST',
			url: 'http://csse371-04.csse.rose-hulman.edu/index.php?method=register',
			data: '{ "user": "test123", "pass": "123"}',
